help -> mc -h
config -> mc -c
compress -> mc [${compress_mode}${compress_param}] o_path(file / dir) [d_path]
                
mov:    c[23-40], b(fast, recommend)[23-40]
mp4:    c(recommend)[23-40], b[23-40]

m4a:    b[64, 128]

heic:   q[2-31], j(recommend)50
png:    j(recommend)50
jpg:    j(recommend)50

gif:    r(0-1), f{0.1, ... 0.9}
